{
  "compilerOptions": {
    "target": "es2019",
    "useDefineForClassFields": true,
    "lib": ["dom", "es2019"],
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "."
  },
  "include": ["contexts", "components", "services", "types.ts", "Constants.ts"]
}
